<%= content_tag(:div,
    id: object.id,
    data: object.data,
    class: object.classname) do %>

    <div class="pb_time_stacked">
      <%= pb_rails("body", props: { text: object.format_time_string, color: "light" }) %>
      <%= pb_rails("body", props: { text: object.format_timezone, color: "light" }) %>
    </div>

<% end %>
